/* Defines which code generator to use */ 
GeneratorModel for sctunit::java {
	/* Refers to the sctunit test 'LightSwitchTest' */
	test LightSwitchTest {
		/* Specify the target location for the generated artifacts. */
		feature Outlet {
			targetProject = "org.yakindu.sct.examples.sctunit.java"
			targetFolder = "src-gen"
		}
		
		feature Naming {
			basePackage = "light.tests"
		}
		
		feature StatechartNaming {
			libraryTargetFolder = "src-gen"
			basePackage = "light"
		}
	}
}
