/* Defines which code generator to use */ 
GeneratorModel for sctunit::c {
	/* Refers to the sctunit testl 'LightSwitchTest' */
	test LightSwitchTest {
		/* Specify the target location for the generated artifacts. */
		feature Outlet {
			targetProject = "org.yakindu.sct.examples.sctunit.c"
			targetFolder = "src-gen"
		}
	}
}
